@prefix event: <http://fedora.info/definitions/v4/event#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .

<http://fedora.info/definitions/v4/event#> a owl:Ontology ;
    rdfs:label "Fedora Event Ontology" ;
    rdfs:comment "An ontology to describe the events emitted by a Fedora repository" ;
    rdfs:seeAlso <https://wiki.duraspace.org/display/FEDORAAPI/Messaging+SPI> ;
    dcterms:publisher <http://fedorarepository.org/> ;
    owl:versionInfo "v0" .


event:ResourceCreation a rdfs:Class ;
    rdfs:label "resource creation" ;
    rdfs:comment "A new resource was created." ;
    vs:term_status "stable" .

event:ResourceDeletion a rdfs:Class ;
    rdfs:label "resource deletion" ;
    rdfs:comment "A resource was deleted." ;
    vs:term_status "stable" .

event:ResourceModification a rdfs:Class ;
    rdfs:label "resource modification" ;
    rdfs:comment "A resource was modified." ;
    vs:term_status "stable" .

